class Player:
    def __init__(self, name):
        self.name = name
        self.vitality = 100
        self.resources = 1000
        self.enhancements = set()
        self.self_awareness = 0
        self.connections = {"NuraTech": 0, "Resistance": 0, "Outskirts": 0}
        self.allegiance = "Unaffiliated"
        self.insights = set()
        self.self_image = "Uncertain"
        self.true_nature = "Unknown"

    def add_enhancement(self, enhancement):
        self.enhancements.add(enhancement)
        if "Neural" in enhancement:
            self.self_awareness += 1

    def gain_insight(self, insight):
        self.insights.add(insight)

    def __str__(self):
        return f"Name: {self.name}, Vitality: {self.vitality}, Resources: {self.resources}, Enhancements: {self.enhancements}, Self-Awareness: {self.self_awareness}, Connections: {self.connections}, Allegiance: {self.allegiance}, Self-Image: {self.self_image}, True Nature: {self.true_nature}"

def start_game():
    print("Welcome to 'Identity Crisis'")
    name = input("What is your name? ")
    player = Player(name)
    print(f"Hello, {player.name}. You find yourself in a world where the boundaries between human, machine, and something in-between are blurred.")
    main_loop(player)

def main_loop(player):
    day_count = 0
    while player.vitality > 0:
        day_count += 1
        print(f"\n--- Day {day_count} ---")
        print("1. Explore the City")
        print("2. Visit NuraTech")
        print("3. Contact the Resistance")
        print("4. Venture to the Outskirts")
        print("5. Personal Reflection")

        choice = input("What would you like to do? ")

        if choice == "1":
            explore_city(player)
        elif choice == "2":
            visit_nuratech(player)
        elif choice == "3":
            contact_resistance(player)
        elif choice == "4":
            venture_outskirts(player)
        elif choice == "5":
            personal_reflection(player)
        else:
            print("You decide to rest for the day.")
            return

        if day_count % 5 == 0:
            print("\nYou have a moment of calm in your journey.")
            print("1. Reflect on your identity")
            print("2. Continue on your path")

            choice = input("How do you spend this moment? ")

            if choice == "1":
                contemplate_identity(player)
            if choice == "2":
                print("You move forward, leaving little time for introspection or connection.")
                return
            else:
                print("You decide to rest for the day.")
                return

        update_true_nature(player)

        if endgame_conditions_met(player):
            final_confrontation(player)
            break

    if player.vitality <= 0:
        print("Your journey comes to an end, but in this world, endings are just new beginnings...")
        restart_game()

def explore_city(player):
    print("\nYou wander the bustling streets of the city.")
    print("1. Visit the Black Market")
    print("2. Investigate NuraTech Propaganda")
    print("3. Look for Resistance Activity")
    print("4. Talk to the Struggling Synthetics")
    print("5. Explore the Tech District")

    choice = input("Where do you go? ")

    if choice == "1":
        black_market(player)
    elif choice == "2":
        nuratech_propaganda(player)
    elif choice == "3":
        resistance_activity(player)
    elif choice == "4":
        struggling_synthetics(player)
    elif choice == "5":
        tech_district(player)
    else:
        print("You wander aimlessly, taking in the sights and sounds of the city.")
        return
def black_market(player):
    print("\nYou enter the shadowy world of the black market.")
    print("1. Purchase Experimental Enhancements")
    print("2. Buy Classified Information")
    print("3. Take on a Risky Job")
    print("4. Trade in Synthetic Parts")
    print("5. Look for the Scarce NuraTech Drug")

    choice = input("What do you want to do? ")

    if choice == "1":
        experimental_enhancements(player)
    elif choice == "2":
        classified_information(player)
    elif choice == "3":
        risky_job(player)
    elif choice == "4":
        synthetic_parts_trade(player)
    elif choice == "5":
        nuratech_drug_search(player)
    else:
        print("You leave the black market, overwhelmed by its offerings.")
        return

def experimental_enhancements(player):
    print("\nA shady dealer approaches you.")
    print("Dealer: 'Looking for an edge? I've got some cutting-edge tech that'll blow your mind.'")
    
    print("1. Neural Pathway Amplifier")
    print("2. Synthetic Muscle Fibers")
    print("3. Quantum Processing Unit")
    print("4. Chameleon Skin Graft")
    print("5. Empathy Enhancer")
    
    choice = input("Which enhancement interests you? ")
    
    if choice == "1":
        print("Dealer: 'The Neural Pathway Amplifier will make you think faster than any human or machine.'")
        if player.resources >= 500:
            player.add_enhancement("Neural Pathway Amplifier")
            player.resources -= 500
            player.self_awareness += 2
            print("You feel your thoughts accelerating as the device integrates with your brain.")
            return
        else:
            print("You don't have enough resources for this enhancement.")
            return
    elif choice == "2":
        print("Dealer: 'Synthetic Muscle Fibers will make you stronger than ten men combined.'")
        if player.resources >= 400:
            player.add_enhancement("Synthetic Muscle Fibers")
            player.resources -= 400
            player.vitality += 20
            print("You feel a surge of strength as the fibers weave into your muscles.")
            return
        else:
            print("You don't have enough resources for this enhancement.")
            return
    elif choice == "3":
        print("Dealer: 'The Quantum Processing Unit is top-of-the-line. You'll process information like a supercomputer.'")
        if player.resources >= 600:
            player.add_enhancement("Quantum Processing Unit")
            player.resources -= 600
            player.self_awareness += 3
            print("Your perception of reality shifts as quantum calculations become second nature.")
            return
        else:
            print("You don't have enough resources for this enhancement.")
            return
    elif choice == "4":
        print("Dealer: 'The Chameleon Skin Graft allows you to blend in with any environment.'")
        if player.resources >= 550:
            player.add_enhancement("Chameleon Skin Graft")
            player.resources -= 550
            print("Your skin tingles as it gains the ability to change color and texture.")
            return
        else:
            print("You don't have enough resources for this enhancement.")
            return
    elif choice == "5":
        print("Dealer: 'The Empathy Enhancer lets you read emotions like an open book.'")
        if player.resources >= 450:
            player.add_enhancement("Empathy Enhancer")
            player.resources -= 450
            player.self_awareness += 2
            print("You suddenly become acutely aware of the emotions of those around you.")
            return
        else:
            print("You don't have enough resources for this enhancement.")
            return
    else:
        print("You decide not to purchase any enhancements today.")
        return
def classified_information(player):
    print("\nAn information broker sidles up to you.")
    print("Broker: 'Psst, looking for some insider info? I've got secrets that could change everything you think you know.'")
    
    print("1. NuraTech's Secret Projects")
    print("2. Resistance Movement Plans")
    print("3. Truth about the Outskirts")
    print("4. Synthetic Origins")
    print("5. Global Power Dynamics")
    
    choice = input("What information do you want to buy? ")
    
    if choice == "1":
        if player.resources >= 300:
            player.gain_insight("NuraTech's True Agenda")
            player.resources -= 300
            print("You learn about NuraTech's plans to create a hive-mind AI using harvested human consciousness.")
            player.self_awareness += 2
            player.connections["NuraTech"] -= 5
            return
        else:
            print("You don't have enough resources for this information.")
            return
    elif choice == "2":
        if player.resources >= 250:
            player.gain_insight("Resistance's Internal Struggles")
            player.resources -= 250
            print("You discover that the Resistance is divided on whether to destroy or repurpose NuraTech's AI.")
            player.connections["Resistance"] += 10
            player.self_awareness += 1
            return
        else:
            print("You don't have enough resources for this information.")
            return
    elif choice == "3":
        if player.resources >= 350:
            player.gain_insight("Outskirts' Hidden Knowledge")
            player.resources -= 350
            print("You learn that the Outskirts have developed a symbiotic relationship between humans and nature-based AI.")
            player.connections["Outskirts"] += 15
            player.self_awareness += 2
            return
        else:
            print("You don't have enough resources for this information.")
            return
    elif choice == "4":
        if player.resources >= 400:
            player.gain_insight("True Nature of Synthetics")
            player.resources -= 400
            print("You uncover the shocking truth about synthetic origins and their connection to human consciousness.")
            player.self_awareness += 3
            if player.true_nature == "Advanced Synthetic" or player.true_nature == "Reclaimed Synthetic":
                print("This revelation shakes you to your core, making you question your entire existence.")
            return
        else:
            print("You don't have enough resources for this information.")
            return
    elif choice == "5":
        if player.resources >= 500:
            player.gain_insight("Global Power Structures")
            player.resources -= 500
            print("You gain access to information about the hidden powers controlling NuraTech, the Resistance, and even the Outskirts.")
            player.self_awareness += 2
            player.connections["NuraTech"] -= 5
            player.connections["Resistance"] -= 5
            player.connections["Outskirts"] -= 5
            return
        else:
            print("You don't have enough resources for this information.")
            return
    else:
        print("You decide not to purchase any information today.")
        print("The broker disappears into the shadows, leaving you to ponder the implications of what you've learned.")
        return
def risky_job(player):
    print("\nA mysterious figure approaches you with a job offer.")
    print("Figure: 'I've got some high-risk, high-reward jobs. Interested in making some serious resources?'")
    
    print("1. Infiltrate NuraTech Facility")
    print("2. Sabotage Resistance Operation")
    print("3. Retrieve Artifact from the Outskirts")
    print("4. Smuggle Synthetic Parts")
    print("5. Hack into City Mainframe")
    
    choice = input("Which job do you want to take? ")
    
    if choice == "1":
        print("Figure: 'Sneak into NuraTech's R&D department and steal their latest prototype.'")
        if "Stealth Module" in player.enhancements:
            player.resources += 1000
            player.connections["NuraTech"] -= 20
            player.gain_insight("NuraTech's Advanced Prototypes")
            print("You successfully infiltrate NuraTech and steal their prototype, gaining valuable insights and resources.")
            return
        else:
            player.vitality -= 30
            player.connections["NuraTech"] -= 10
            print("Your infiltration attempt fails, and you barely escape with your life.")
            return
    elif choice == "2":
        print("Figure: 'The Resistance is planning a major operation. Sabotage it, and you'll be well compensated.'")
        player.resources += 800
        player.connections["Resistance"] -= 25
        player.gain_insight("Resistance's Weaknesses")
        print("You successfully sabotage the Resistance operation, earning a substantial reward but compromising your standing with them.")
        return
    elif choice == "3":
        print("Figure: 'There's a powerful artifact hidden in the Outskirts. Retrieve it, and you'll be set for life.'")
        if "Wilderness Survival" in player.enhancements:
            player.resources += 1200
            player.connections["Outskirts"] -= 15
            player.gain_insight("Ancient Technological Wisdom")
            print("You successfully retrieve the artifact, gaining immense wealth and knowledge, but disturbing the Outskirts' peace.")
            return
        else:
            player.vitality -= 25
            print("The dangers of the Outskirts prove too much, and you return empty-handed and injured.")
            return
    elif choice == "4":
        print("Figure: 'We need someone to move some... sensitive synthetic components across city borders.'")
        if "Chameleon Skin Graft" in player.enhancements:
            player.resources += 900
            player.gain_insight("Synthetic Black Market")
            print("Your ability to blend in allows you to smuggle the parts successfully, earning a hefty payment.")
            return
        else:
            player.connections["NuraTech"] -= 15
            player.connections["Resistance"] += 10
            print("You're caught by authorities, but manage to escape. The experience gives you insights into both sides of the conflict.")
            return
    elif choice == "5":
        print("Figure: 'The city's mainframe holds valuable data. Get in, get the data, get out.'")
        if "Quantum Processing Unit" in player.enhancements:
            player.resources += 1500
            player.gain_insight("City-Wide Data Patterns")
            player.self_awareness += 2
            print("Your enhanced processing capabilities allow you to hack the mainframe and escape undetected, with a wealth of valuable data.")
            return
        else:
            player.vitality -= 20
            player.connections["NuraTech"] -= 5
            print("The mainframe's security proves too advanced. You trigger alarms and barely escape, learning the hard way about the city's cyber defenses.")
            return
    else:
        print("You decide the risks outweigh the potential rewards and walk away from the mysterious figure.")
        print("The figure disappears into the shadows, leaving you to contemplate the consequences of your actions.")
        return
def synthetic_parts_trade(player):
    print("\nYou enter a dimly lit shop filled with synthetic body parts.")
    print("Shopkeeper: 'Welcome! Looking to upgrade? Or perhaps... downgrade? I've got all sorts of parts here.'")
    
    print("1. Sell Excess Parts")
    print("2. Buy Reclaimed Synthetic Parts")
    print("3. Inquire about Rare Components")
    print("4. Examine Custom-Made Parts")
    print("5. Trade Parts with Other Customers")
    
    choice = input("What would you like to do? ")
    
    if choice == "1":
        if len(player.enhancements) > 0:
            print("Your available parts:")
            for i, enhancement in enumerate(player.enhancements, 1):
                print(f"{i}. {enhancement}")
            sell_choice = input("Which part would you like to sell? (Enter the number or 'none') ")
            if sell_choice.lower() != 'none' and sell_choice.isdigit():
                index = int(sell_choice) - 1
                if 0 <= index < len(player.enhancements):
                    part = list(player.enhancements)[index]
                    player.enhancements.remove(part)
                    player.resources += 200
                    print(f"You sell your {part} for 200 resources.")
                    player.self_awareness += 1
                    return
                else:
                    print("Invalid choice. No transaction made.")
                    return
            else:
                print("You decide not to sell any parts.")
                return
        else:
            print("You don't have any parts to sell.")
            return
    elif choice == "2":
        print("Available reclaimed parts:")
        print("1. Refurbished Optical Unit (300 resources)")
        print("2. Second-hand Neural Interface (400 resources)")
        print("3. Recycled Synthetic Limb (350 resources)")
        buy_choice = input("Which part would you like to buy? (Enter the number or 'none') ")
        if buy_choice == "1" and player.resources >= 300:
            player.add_enhancement("Refurbished Optical Unit")
            player.resources -= 300
            print("You purchase the Refurbished Optical Unit, gaining enhanced vision but with occasional glitches.")
            return
        elif buy_choice == "2" and player.resources >= 400:
            player.add_enhancement("Second-hand Neural Interface")
            player.resources -= 400
            print("You buy the Second-hand Neural Interface, improving your reaction time but with some sensory feedback issues.")
            return
        elif buy_choice == "3" and player.resources >= 350:
            player.add_enhancement("Recycled Synthetic Limb")
            player.resources -= 350
            print("You acquire the Recycled Synthetic Limb, increasing your strength but with limited fine motor control.")
            return
        else:
            print("You decide not to purchase any reclaimed parts.")
            return
    elif choice == "3":
        print("Shopkeeper: 'Rare components? I might have something special in the back...'")
        print("The shopkeeper returns with a mysterious device.")
        print("Shopkeeper: 'This is a prototype Quantum Entanglement Module. It's... not entirely stable.'")
        if player.resources >= 1000:
            purchase = input("Would you like to buy it for 1000 resources? (yes/no) ")
            if purchase.lower() == "yes":
                player.add_enhancement("Quantum Entanglement Module")
                player.resources -= 1000
                player.self_awareness += 3
                print("You acquire the Quantum Entanglement Module. Your perception of reality begins to shift in unexpected ways.")
                return
            else:
                print("You decide the risk isn't worth it.")
                return
        else:
            print("You don't have enough resources for such a rare component.")
            return
    elif choice == "4":
        print("Shopkeeper: 'Interested in something unique? I can craft a part tailored just for you.'")
        print("1. Emotion Regulator")
        print("2. Memory Enhancer")
        print("3. Synthetic Skin with Chameleon Properties")
        custom_choice = input("Which custom part interests you? ")
        if custom_choice in ["1", "2", "3"] and player.resources >= 600:
            player.resources -= 600
            if custom_choice == "1":
                player.add_enhancement("Custom Emotion Regulator")
                print("You receive a Custom Emotion Regulator. It allows you to control your emotional responses, but at what cost to your humanity?")
                return
            elif custom_choice == "2":
                player.add_enhancement("Custom Memory Enhancer")
                print("You're fitted with a Custom Memory Enhancer. Your recall becomes perfect, but you find it harder to forget painful memories.")
                return
            else:
                player.add_enhancement("Custom Chameleon Skin")
                print("You're grafted with Custom Chameleon Skin. You can blend in anywhere, but you begin to question your true appearance.")
                return
            player.self_awareness += 2
        else:
            print("You decide against custom parts for now.")
            return
    elif choice == "5":
        if len(player.enhancements) > 0:
            print("You meet another customer interested in trading parts.")
            print("They offer a 'Synaptic Accelerator' in exchange for one of your enhancements.")
            trade = input("Would you like to trade? (yes/no) ")
            if trade.lower() == "yes":
                traded_part = player.enhancements.pop()
                player.add_enhancement("Synaptic Accelerator")
                print(f"You trade your {traded_part} for a Synaptic Accelerator. Your thought processes speed up, but you feel a sense of loss for the traded part.")
                player.self_awareness += 1
                return
            else:
                print("You decide to keep your current enhancements.")
                return
        else:
            print("You don't have any parts to trade.")
            return
    else:
        print("You leave the shop without making any transactions.")
        print("As you exit the shop, you can't help but reflect on how each part, bought or sold, changes who you are.")
        return
def nuratech_drug_search(player):
    print("\nYou discreetly ask around about the scarce NuraTech drug.")
    print("Dealer: 'That stuff's hard to come by these days. NuraTech's got a tight grip on the supply.'")
    
    print("1. Try to Buy the Drug")
    print("2. Inquire About Its Effects")
    print("3. Ask About Alternatives")
    print("4. Investigate the Supply Chain")
    print("5. Offer to Help Distribute")
    
    choice = input("What do you want to do? ")
    
    if choice == "1":
        if player.resources >= 500:
            player.resources -= 500
            player.vitality += 10
            player.add_enhancement("NuraTech Drug")
            print("You manage to acquire a small dose of the drug. It stabilizes your systems, but for how long?")
            player.gain_insight("NuraTech Drug Experience")
            return
        else:
            print("You don't have enough resources to buy the drug.")
            return
    elif choice == "2":
        print("Dealer: 'It's like a lifeline for older synths. Keeps them functioning, but it's a double-edged sword.'")
        player.gain_insight("NuraTech Drug Dependency")
        print("You learn about the drug's addictive nature and its critical role in synthetic survival.")
        return
    elif choice == "3":
        print("Dealer: 'Alternatives? Some say the Outskirts have found a way, but that's just a rumor.'")
        player.gain_insight("Potential Drug Alternatives")
        player.connections["Outskirts"] += 5
        return
    elif choice == "4":
        if "Hacking Interface" in player.enhancements:
            print("You use your hacking skills to trace the drug's distribution network.")
            player.gain_insight("NuraTech Drug Supply Chain")
            player.connections["NuraTech"] -= 10
            return
        else:
            print("Without the right tech, you can't dig deep into the supply chain.")
            return
    elif choice == "5":
        if player.connections["NuraTech"] > 20:
            print("Your NuraTech connections allow you to become a low-level distributor.")
            player.resources += 300
            player.connections["Resistance"] -= 15
            player.gain_insight("NuraTech Drug Trade")
            return
        else:
            print("The dealer doesn't trust you enough to involve you in distribution.")
            return
    else:
        print("You decide to leave the topic alone for now, wary of getting too involved.")
        print("As you walk away, you ponder the implications of the drug's scarcity and its impact on synthetic life.")
        return
def nuratech_propaganda(player):
    print("\nYou come across a large NuraTech billboard showcasing their latest innovations.")
    print("The slogan reads: 'NuraTech: Bridging Humanity and Technology for a Brighter Future'")
    
    print("1. Analyze the Message")
    print("2. Look for Hidden Meanings")
    print("3. Observe Public Reaction")
    print("4. Interact with the Holographic Display")
    print("5. Investigate the Billboard's Construction")
    
    choice = input("What do you want to do? ")
    
    if choice == "1":
        print("You notice how the message appeals to both human desires and technological progress.")
        player.self_awareness += 1
        player.gain_insight("NuraTech's Public Image")
        return
    elif choice == "2":
        if "Pattern Recognition" in player.enhancements:
            print("You detect subtle visual cues that seem to trigger subconscious responses in viewers.")
            player.gain_insight("NuraTech's Subliminal Messaging")
            player.connections["NuraTech"] -= 5
            return
        else:
            print("You sense there might be more to the message, but can't quite figure it out.")
            return
    elif choice == "3":
        print("You see a mix of awe, skepticism, and indifference among the passersby.")
        player.gain_insight("Public Opinion on NuraTech")
        player.self_awareness += 1
        return
    elif choice == "4":
        print("You interact with the holographic display, which showcases NuraTech's latest products.")
        if "Quantum Processing Unit" in player.enhancements:
            print("Your enhanced processing allows you to spot inconsistencies in the product claims.")
            player.gain_insight("NuraTech's Exaggerated Promises")
            return
        else:
            print("The sleek presentation leaves you impressed with NuraTech's innovations.")
            player.connections["NuraTech"] += 5
            return
    elif choice == "5":
        if "Enhanced Vision" in player.enhancements:
            print("Your enhanced vision reveals hidden transmitters embedded in the billboard.")
            player.gain_insight("NuraTech's Surveillance Network")
            player.connections["Resistance"] += 5
            return
        else:
            print("The billboard seems to be a standard high-tech display.")
            return
    else:
        print("You walk past the billboard, trying not to let it influence you.")
        print("As you move on, you ponder the impact of such pervasive advertising on society's perception of technology.")
        return
def resistance_activity(player):
    print("\nYou notice some subtle signs that might indicate Resistance activity in the area.")
    print("A figure in the shadows seems to be watching you.")
    
    print("1. Approach the Figure")
    print("2. Look for Resistance Symbols")
    print("3. Pretend You Haven't Noticed")
    print("4. Use Enhanced Senses to Scan the Area")
    print("5. Leave an Encrypted Message")
    
    choice = input("What do you do? ")
    
    if choice == "1":
        print("Figure: 'You seem aware. Are you one of us, or one of them?'")
        response = input("How do you respond? (1. 'I'm with you' / 2. 'I'm my own person' / 3. Stay silent) ")
        if response == "1":
            player.connections["Resistance"] += 10
            print("The figure nods approvingly and shares some information about NuraTech's recent activities.")
            player.gain_insight("NuraTech's Recent Operations")
            return
        elif response == "2":
            player.self_awareness += 1
            print("The figure seems intrigued by your response and offers you a chance to prove yourself.")
            player.gain_insight("Resistance Recruitment Methods")
            return
        else:
            print("The figure disappears into the shadows, leaving you wondering about the encounter.")
            return
    elif choice == "2":
        if "Enhanced Vision" in player.enhancements:
            print("You spot a hidden Resistance symbol, revealing the location of a secret meeting.")
            player.gain_insight("Resistance Communication Methods")
            player.connections["Resistance"] += 5
            return
        else:
            print("You search for symbols but find nothing conclusive.")
            return
    elif choice == "3":
        print("You continue on your way, but can't shake the feeling of being watched.")
        player.self_awareness += 1
        return
    elif choice == "4":
        if "Quantum Sensing" in player.enhancements:
            print("Your enhanced senses detect a hidden underground facility nearby.")
            player.gain_insight("Resistance Hidden Base")
            player.connections["Resistance"] += 15
            return
        else:
            print("You try to sense more but lack the necessary enhancements.")
            return
    elif choice == "5":
        if "Cryptography Module" in player.enhancements:
            print("You leave an encrypted message for the Resistance. Soon after, you receive a coded response.")
            player.gain_insight("Resistance Encryption Techniques")
            player.connections["Resistance"] += 10
            return
        else:
            print("You attempt to leave a message, but worry it might be intercepted by NuraTech.")
            return
    else:
        print("Your indecision causes the moment to pass, and the figure disappears.")
        print("As you reflect on this encounter, you ponder the complexities of loyalty and identity in this fractured world.")
        return
def struggling_synthetics(player):
    print("\nYou encounter a group of older-model synthetics, clearly struggling without access to NuraTech's drug.")
    print("1. Offer Assistance")
    print("2. Inquire About Their Condition")
    print("3. Observe from a Distance")
    print("4. Share Information About Alternatives")
    print("5. Recruit Them for the Resistance")
    
    choice = input("How do you approach the situation? ")
    
    if choice == "1":
        if "NuraTech Drug" in player.enhancements:
            print("You share some of your supply, providing temporary relief to the synthetics.")
            player.connections["Resistance"] += 15
            player.gain_insight("Synthetic Dependency")
            return
        else:
            print("You want to help but lack the means to do so, leaving you feeling helpless.")
            return
    elif choice == "2":
        print("Synthetic: 'We're... malfunctioning. NuraTech's 'maintenance' is becoming scarce.'")
        player.gain_insight("NuraTech's Control Methods")
        return
    elif choice == "3":
        print("You watch discreetly, noting the various signs of deterioration in the older models.")
        player.self_awareness += 1
        return
    elif choice == "4":
        if "Potential Drug Alternatives" in player.insights:
            print("You share information about potential alternatives from the Outskirts.")
            player.connections["Outskirts"] += 10
            player.connections["Resistance"] += 5
            return
        else:
            print("You wish you knew of alternatives to help them.")
            return
    elif choice == "5":
        if player.connections["Resistance"] > 20:
            print("You convince some of the synthetics to join the Resistance for a chance at freedom.")
            player.connections["Resistance"] += 20
            player.gain_insight("Synthetic Motivation")
            return
        else:
            print("The synthetics are too wary to trust you with such a proposition.")
            return
    else:
        print("You walk away, the image of the struggling synthetics lingering in your mind.")
        return

