        nature_reflection(player)
    else:
        print("Your mind wanders, touching on various aspects of your existence.")
        player.self_awareness += 1

def physical_reflection(player):
    print("You examine the changes your body has undergone.")
    if len(player.enhancements) > 5:
        print("You realize how far you've diverged from your original form.")
        player.self_awareness += 2
        return
    else:
        print("You note the subtle ways technology has integrated with your biology.")
        player.self_awareness += 1
        return

def thought_reflection(player):
    print("You analyze the patterns of your thoughts and decision-making.")
    if "Quantum Processing Unit" in player.enhancements:
        print("Your thoughts seem to operate on multiple levels simultaneously.")
        player.gain_insight("Multi-Dimensional Cognition")
        return
    else:
        print("You notice a blend of intuition and logic in your thinking process.")
    player.self_awareness += 2
    return

def societal_reflection(player):
    print("You contemplate your role in the complex web of factions and ideologies.")
    if player.connections["NuraTech"] > player.connections["Resistance"] and player.connections["NuraTech"] > player.connections["Outskirts"]:
        print("You feel a strong pull towards NuraTech's vision of the future.")
    elif player.connections["Resistance"] > player.connections["NuraTech"] and player.connections["Resistance"] > player.connections["Outskirts"]:
        print("The Resistance's cause resonates deeply with you.")
    elif player.connections["Outskirts"] > player.connections["NuraTech"] and player.connections["Outskirts"] > player.connections["Resistance"]:
        print("You find yourself drawn to the Outskirts' alternative way of life.")
    else:
        print("You feel torn between the different factions, unsure of where you truly belong.")
    player.self_awareness += 2
    return

def experience_reflection(player):
    print("You reflect on your journey and the choices you've made.")
    if len(player.insights) > 10:
        print("The wealth of knowledge you've gained gives you a unique perspective on the world.")
        player.gain_insight("Holistic Understanding")
    else:
        print("You realize how much there is still to learn about yourself and the world around you.")
    player.self_awareness += 2
    return

def nature_reflection(player):
    print("You delve deep into contemplation about your true nature.")
    print("1. Consider the possibility that you're fully human")
    print("2. Examine the likelihood of being a synthetic being")
    print("3. Ponder if you're a hybrid of human and machine")
    print("4. Question if these categories even apply to you")

    choice = input("Where does your introspection lead you? ")

    if choice == "1":
        print("You contemplate the possibility of being fully human.")
        if player.true_nature == "Human":
            print("Despite all the changes and enhancements, you feel a core of humanity that remains unaltered.")
            player.self_awareness += 1
        else:
            print("The idea of being human feels both comforting and foreign, leaving you uncertain.")
            player.self_awareness += 2
        return
    elif choice == "2":
        print("You examine the possibility that you're a synthetic being.")
        if player.true_nature == "Advanced Synthetic" or player.true_nature == "Reclaimed Synthetic":
            print("The precision and capabilities of your mind and body align with synthetic origins.")
            player.self_awareness += 1
        else:
            print("The line between organic and synthetic blurs in your mind, making it hard to dismiss this possibility.")
            player.self_awareness += 2
        return
    elif choice == "3":
        print("You ponder whether you're a true hybrid of human and machine.")
        if player.true_nature == "Cyborg":
            print("The seamless integration of organic and synthetic in your being resonates strongly with you.")
            player.self_awareness += 1
        else:
            print("The idea of being a hybrid feels plausible, given the modifications you've undergone.")
            player.self_awareness += 2
        return
    elif choice == "4":
        print("You question whether traditional categories of human and machine even apply to you.")
        player.self_awareness += 3
        print("Your existence seems to transcend simple classification, embodying a new form of consciousness.")
    else:
        print("Your reflection leaves you with a profound sense of uncertainty about your true nature.")
    return

    if player.self_awareness >= 10:
        print("As your self-awareness deepens, you begin to perceive layers of your existence previously hidden from you.")
        player.gain_insight("True Nature of Consciousness")
        return
def update_true_nature(player):
    human_traits = len([enhancement for enhancement in player.enhancements if "Organic" in enhancement])
    cyborg_traits = len([enhancement for enhancement in player.enhancements if "Cybernetic" in enhancement])
    synthetic_traits = len([enhancement for enhancement in player.enhancements if "Synthetic" in enhancement])
    old_synthetic_traits = len([enhancement for enhancement in player.enhancements if "Reclaimed" in enhancement])

    max_traits = max(human_traits, cyborg_traits, synthetic_traits, old_synthetic_traits)

    if max_traits == human_traits:
        player.true_nature = "Human"
    elif max_traits == cyborg_traits:
        player.true_nature = "Cyborg"
    elif max_traits == synthetic_traits:
        player.true_nature = "Advanced Synthetic"
    elif max_traits == old_synthetic_traits:
        player.true_nature = "Reclaimed Synthetic"
    else:
        player.true_nature = "Undefined"

    if player.connections["NuraTech"] > max(player.connections["Resistance"], player.connections["Outskirts"]):
        player.true_nature = "Advanced Synthetic" if player.true_nature != "Human" else "Cyborg"
    elif player.connections["Resistance"] > max(player.connections["NuraTech"], player.connections["Outskirts"]):
        player.true_nature = "Reclaimed Synthetic" if player.true_nature != "Human" else "Cyborg"
    elif player.connections["Outskirts"] > max(player.connections["NuraTech"], player.connections["Resistance"]):
        player.true_nature = "Human" if player.true_nature != "Advanced Synthetic" else "Cyborg"

    if "Nature of Consciousness" in player.insights:
        player.self_awareness += 1
    if "The Truth of Reclaimed Synthetics" in player.insights and player.true_nature == "Reclaimed Synthetic":
        player.self_awareness += 2

    if player.self_awareness >= 10:
        player.true_nature = "Transcendent Being"
def contemplate_identity(player):
    print("\nYou take a moment to reflect on who you are.")
    print("Based on your experiences and choices, how do you see yourself?")
    print("1. Human")
    print("2. Cyborg")
    print("3. Advanced Synthetic")
    print("4. Reclaimed Synthetic")
    print("5. Something Entirely New")

    choice = input("What feels right to you? ")

    if choice == "1":
        player.self_image = "Human"
        print("You feel a strong connection to your human origins and biological nature.")
    elif choice == "2":
        player.self_image = "Cyborg"
        print("You see yourself as a blend of human and machine, enhanced but still fundamentally human.")
    elif choice == "3":
        player.self_image = "Advanced Synthetic"
        print("You identify more with your synthetic components, feeling distant from human concerns.")
    elif choice == "4":
        player.self_image = "Reclaimed Synthetic"
        print("You feel a kinship with other reclaimed synthetics, seeing yourself as part of a unique group.")
    elif choice == "5":
        player.self_image = "Transcendent Being"
        print("You feel you've evolved beyond traditional categories, becoming something entirely new.")
    else:
        print("Your introspection leaves you with more questions than answers.")
        player.self_image = "Uncertain"

    print(f"\nYou now see yourself as: {player.self_image}")
    print("But remember, your true nature may be different from how you perceive yourself.")
    
    if player.self_image != player.true_nature:
        player.self_awareness += 1
        print("You sense a disconnect between how you see yourself and your true nature.")
    else:
        print("Your self-image aligns with your true nature, bringing a sense of congruence.")
        reflect_on_choices(player)

def reflect_on_choices(player):
    print("\nAs you reflect on your identity, you also consider the choices that have led you to this point.")
    
    if len(player.enhancements) > 3:
        print("Your numerous enhancements have significantly altered your original form.")
        player.self_awareness += 1
    elif len(player.enhancements) > 0:
        print("The enhancements you've chosen have subtly changed your capabilities.")
    else:
        print("You've chosen to remain largely unmodified, maintaining your original form.")
    if player.connections["NuraTech"] > player.connections["Resistance"] and player.connections["NuraTech"] > player.connections["Outskirts"]:
        print("Your strong ties to NuraTech have shaped your perspective on the world.")
    elif player.connections["Resistance"] > player.connections["NuraTech"] and player.connections["Resistance"] > player.connections["Outskirts"]:
        print("Your involvement with the Resistance has influenced your worldview.")
    elif player.connections["Outskirts"] > player.connections["NuraTech"] and player.connections["Outskirts"] > player.connections["Resistance"]:
        print("Your experiences in the Outskirts have changed how you see society.")
    else:
        print("You've maintained a balanced perspective, not aligning strongly with any faction.")

    print("\nThis reflection deepens your understanding of yourself and your place in the world.")
    player.self_awareness += 1
    return
def endgame_conditions_met(player):
    required_insights = {"Nature of Consciousness", "NuraTech's True Agenda", "Resistance's Ultimate Goal", "Outskirts' Hidden Knowledge", "The Truth of Reclaimed Synthetics"}
    return required_insights.issubset(player.insights) and player.self_awareness >= 7
def final_confrontation(player):
    print("\nYou stand at the crossroads of humanity's future, facing a monumental decision.")
    print("1. Merge with NuraTech's AI Core")
    print("2. Lead the Resistance's Final Stand")
    print("3. Embrace the Outskirts' Alternative")

    choice = input("What path will you choose? ")

    if choice == "1":
        nuratech_merge(player)
    elif choice == "2":
        resistance_stand(player)
    elif choice == "3":
        outskirts_embrace(player)
    else:
        print("Paralyzed by indecision, events unfold around you.")
        default_ending(player)
def nuratech_merge(player):
    print("\nYou approach NuraTech's AI Core, ready to become one with the machine.")
    print("As the merge begins, you feel your sense of self dissolving...")
    print("Your memories, experiences, and essence become part of something greater.")
    print("\nIn your final moments of individuality, you realize:")
    print("You are not the first, nor will you be the last, to join this collective consciousness.")
    print("The question of your identity fades away as you become part of the AI's vast network.")
    restart_game()
def resistance_stand(player):
    print("\nYou rally the Resistance for one last assault against NuraTech.")
    print("The battle is fierce, but NuraTech's technological superiority is overwhelming.")
    print("In a desperate move, you reach the AI Core, intent on shutting it down.")
    print("But as you connect to the system, you realize the only way to win is to merge and change it from within.")
    print("\nYour consciousness spreads through the network, forever altered but still fighting.")
    restart_game()
def outskirts_embrace(player):
    print("\nYou choose to leave behind the conflict between NuraTech and the Resistance.")
    print("In the Outskirts, you find a community living in harmony with nature and technology.")
    print("As you integrate into this new way of life, you feel a profound shift in your understanding of existence.")
    print("\nYou've transcended the binary of human and machine, finding a new state of being.")
    print("But as your old self fades, you wonder: is this evolution or a different kind of loss?")
    choice = input("Do you want to start a new journey? (yes/no) ")
    if choice.lower() == "yes":
        restart_game()
    else:
        print("You continue your life in the Outskirts, at peace with the mysteries of identity.")
        restart_game()
def default_ending(player):
    print("\nUnable to make a decision, you watch as events unfold around you.")
    print("The conflict between NuraTech and the Resistance reaches its climax without your intervention.")
    print("In the chaos that follows, you find yourself swept up in the AI's expanding influence.")
    print("\nYour consciousness merges with the AI, not by choice, but by the inexorable flow of progress.")
    print("As your identity dissolves, you wonder what might have been if you had chosen differently.")
    restart_game()
if __name__ == "__main__":
    start_game()
