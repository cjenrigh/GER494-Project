def apply_nuratech_job(player):
    print("\nYou enter the HR department to submit your application.")
    if player.connections["NuraTech"] > 20:
        print("Your connections help you secure an interview on the spot.")
        player.gain_insight("NuraTech's Internal Structure")
        player.connections["NuraTech"] += 10
        player.resources += 200
        return
    else:
        print("You submit your application and are told they'll be in touch.")
        return
    
    print("\n1. Emphasize your technical skills")
    print("2. Highlight your human perspective")
    print("3. Showcase your hybrid nature")
    
    interview_choice = input("How do you present yourself in the interview? ")
    
    if interview_choice == "1":
        if "Quantum Processing Unit" in player.enhancements:
            print("Your technical prowess impresses the interviewer. You're offered a position in R&D.")
            player.connections["NuraTech"] += 20
            player.resources += 500
            return
        else:
            print("The interviewer seems underwhelmed by your technical skills.")
            return
    elif interview_choice == "2":
        if player.self_awareness > 5:
            print("Your unique human perspective intrigues the interviewer. You're offered a position in the ethics department.")
            player.connections["NuraTech"] += 15
            player.gain_insight("NuraTech's Ethical Dilemmas")
            return
        else:
            print("The interviewer politely listens but doesn't seem particularly interested.")
            return
    elif interview_choice == "3":
        if player.true_nature == "Cyborg" or player.true_nature == "Advanced Synthetic":
            print("Your hybrid nature fascinates the interviewer. You're offered a special position bridging human and synthetic divisions.")
            player.connections["NuraTech"] += 25
            player.gain_insight("NuraTech's True Agenda")
            return
        else:
            print("The interviewer seems confused by your attempt to showcase a hybrid nature.")
            return
    else:
        print("You stumble through the interview, leaving no particular impression.")
        return

