def contact_resistance(player):
    print("\nYou seek to make contact with the Resistance movement.")
    print("1. Attend a secret meeting")
    print("2. Offer information to a suspected Resistance member")
    print("3. Investigate Resistance graffiti")
    print("4. Hack into suspected Resistance communications")
    print("5. Volunteer at a community center known for Resistance ties")

    choice = input("How do you engage with the Resistance? ")
    
    if choice == "1":
        attend_resistance_meeting(player)
    elif choice == "2":
        offer_info_to_resistance(player)
    elif choice == "3":
        investigate_resistance_graffiti(player)
    elif choice == "4":
        hack_resistance_communications(player)
    elif choice == "5":
        volunteer_community_center(player)
    else:
        print("You hesitate, unsure how to proceed. The Resistance remains elusive for now.")
        return

def attend_resistance_meeting(player):
    print("\nYou receive an encrypted message with coordinates for a clandestine gathering.")
    print("1. Arrive early to scout the area")
    print("2. Bring a friend for safety")
    print("3. Prepare a speech to inspire others")
    print("4. Wear a disguise to avoid detection")
    print("5. Leave a message for the Resistance if you can't attend")

    choice = input("How do you prepare for the meeting? ")
    
    if choice == "1":
        print("You arrive early and notice a few suspicious figures lurking nearby.")
        print("1. Confront the figures")
        print("2. Hide and observe")
        print("3. Leave the area")
        sub_choice = input("What do you do? ")
        if sub_choice == "1":
            print("You confront the figures, and they turn out to be friendly Resistance members.")
            player.connections["Resistance"] += 10
            return
        elif sub_choice == "2":
            print("You observe them discussing plans, gaining valuable insights.")
            player.gain_insight("Resistance Plans")
            return
        elif sub_choice == "3":
            print("You decide it's too risky and leave the area.")
            return
        else:
            print("Invalid choice. You miss the opportunity.")
            return
    elif choice == "2":
        print("You bring a friend, which makes you feel safer.")
        player.connections["Resistance"] += 5
        print("Your friend shares their own insights during the meeting.")
        return
    elif choice == "3":
        print("You prepare a speech, but the moment you arrive, you feel nervous.")
        print("1. Deliver the speech anyway")
        print("2. Ask someone else to speak")
        print("3. Keep your thoughts to yourself")
        speech_choice = input("What do you do? ")
        if speech_choice == "1":
            print("Your speech inspires the crowd, boosting morale!")
            player.connections["Resistance"] += 15
            return
        elif speech_choice == "2":
            print("You ask a more experienced member to speak, which is well-received.")
            player.connections["Resistance"] += 10
            return
        elif speech_choice == "3":
            print("You keep quiet, missing an opportunity to contribute.")
            return
        else:
            print("Invalid choice. You miss the opportunity.")
            return
    elif choice == "4":
        print("You wear a disguise, which helps you blend in with the crowd.")
        player.gain_insight("Resistance Disguise Techniques")
        return
    elif choice == "5":
        print("You leave a message for the Resistance, ensuring they know you support them.")
        player.connections["Resistance"] += 5
        return
    else:
        print("You hesitate and miss the meeting altogether.")
        return

